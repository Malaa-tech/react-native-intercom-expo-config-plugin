{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/winter.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "config-plugin.intercom.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "config_plugin.intercom.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        { "android": { "compileSdkVersion": 33, "targetSdkVersion": 33 } }
      ],
      [
        "react-native-intercom-expo-config-plugin",
        {
          "iosApiKey": "myIosApiKey",
          "androidApiKey": "myAndroidApiKey",
          "appId": "myAppId"
        }
      ]
    ]
  }
}
